<query id="getMenus" action="select">
    <tables>
        <table name="modules" />
    </tables>
    <columns>
        <column name="mid" />
        <column name="module_srl" />
        <column name="browser_title" />
    </columns>
    <conditions>
        <condition operation="in" column="mid" var="mid" filter="string" />
        <condition operation="in" column="modules_srl" var="module_srl" filter="number" />
    </conditions>
</query>
